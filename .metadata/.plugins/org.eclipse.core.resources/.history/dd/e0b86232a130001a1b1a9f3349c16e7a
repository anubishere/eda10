import java.awt.Color;

public class TestGui {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Complex z = new Complex(0, 0);
		Complex c = new Complex(-0.4, 0.4);
		
		for(int i=0; i<8; i++){
			z.mul(z);
			z.add(c);
			System.out.println(z.getRe() + " + " + z.getIm() + "i");
		}
	}

}




