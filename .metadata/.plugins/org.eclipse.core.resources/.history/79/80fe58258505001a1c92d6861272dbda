package inl1;
import se.lth.cs.p.inl1.*;
import se.lth.cs.p.inl1.nbr7.*;

public class Inl1 {

	public static void main(String[] args) {
		
		TextWindow textWindow = new TextWindow("Chiffrering");
		
		
		TextView klarTexten = new TextView("Klartext: ", 4, 70);
		TextView myCiphertext = new TextView("Klartext: ", 4, 70);
		TextView correctCipher = new TextView("Klartext: ", 4, 70);
		
		TextWindow.addView(klarTexten);
		TextWindow.addView(myCiphertext);
		TextWindow.addView(correctCipher);
		for(int i = 0; i < 5; i++) {
			klarTexten.displayText("Burundi");
		}

	}

}
