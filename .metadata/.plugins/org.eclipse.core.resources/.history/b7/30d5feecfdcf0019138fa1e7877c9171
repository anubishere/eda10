package testet;

import java.util.Scanner;

public class Squaretest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
Scanner scan = new Scanner (System.in);
int a, b, c, d, e;
a = scan.nextInt();
b = scan.nextInt();
c = scan.nextInt();
d = scan.nextInt();
e = c + d - (a + b);
c = c + 2;
a = - (e * 2 + c) / 4 * b;
System.out.print(e);
System.out.print(c);
System.out.print(a);
	}
	}


