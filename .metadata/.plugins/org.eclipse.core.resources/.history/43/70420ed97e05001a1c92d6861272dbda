package inl1;

import java.util.Scanner;

public class Ovnin {

	public static void main(String[] args) {

		        Scanner scan = new Scanner(System.in);
		        
		        int counter = 0;
		        int i = 0;
		        int sum = 0;
		        int medelvärde = (sum / counter);
		        
		        while(scan.hasNextInt()){ // går in i loopen om det finns ett nummer
		        	
		            i = scan.nextInt(); // numret scannas in
		             
		            sum = sum + i; // varje gång loopen körs adderas i på sum. så 0 
		            counter++;
		            System.out.println(medelvärde);
		            }
		            
		            
		        }
		        
		    }





